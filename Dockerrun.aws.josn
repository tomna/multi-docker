{
    "AWSEBDockerrunversion" :2, 
    "containerDefinitions": [
        {
            "name" :"client",
            "image" : "limhye/multi-client",
            "hostname": "clinet",
            "essential" : false,
            "memory" : 128
        },
        {
            "name" :"server",
            "image" : "limhye/multi-server",
            "hostname": "api",
            "essential" : false ,
             "memory" : 128
        }
        ,{
            "name" :"worker",
            "image" : "limhye/multi-worker",
            "hostname": "worker",
            "essential" : false ,
             "memory" : 128
        },
        {
            "name" :"nginx",
            "image" : "limhye/multi-nginx",
            "hostname": "nginx",
            "essential" : true,
             "memory" : 128,
            "portMappings" :[
                {
                    "hostPort":80,
                    "containerPort":80
                }
            ],
             "links":  ["client","server"]
        }
        
    ]
}